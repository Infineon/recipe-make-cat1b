                "monitor program {&&_MTB_RECIPE__FINAL_HEX_FILE&&}",
                "monitor &&_MTB_RECIPE__VSCODE_PROGRAM_STATIC_SECT_CMD&&",//prebuild SS//
                "monitor &&_MTB_RECIPE__RESTORE_STATIC_SECT&&",//restore SS//
                "monitor reset init"
